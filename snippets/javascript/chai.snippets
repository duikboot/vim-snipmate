# to.be.ok
snippet chai.ok
	expect(${1:false}).to.be.ok;${3}
snippet chai.eql
	expect(${1:false}).to.eql(${2:true});${3}
snippet chai.iof
	expect(${1:null}).to.be.instanceof(${2:false});${3}
snippet chai.tohave
	expect(${1:null}).to.have.property('${2:property}');${3}
snippet chai.tobea
	expect(${1:null}).to.be.a('${2:array}');${3}

